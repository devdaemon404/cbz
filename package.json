{
  "name": "cbz-neo",
  "version": "1.0.0",
  "description": "",
  "main": "build/server/App.js",
  "scripts": {
    "copy-env": "copyfiles -f ./server/config/*.env ./build/server/config",
    "copy-templates": "copyfiles -f ./server/controllers/html/* ./build/server/controllers/html",
    "copy-files": "npm run copy-env && npm run copy-templates",
    "dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development nodemon --watch 'server/**' --ext 'ts,json' --exec ts-node --project tsconfig.server.json server/App.ts",
    "prod-test": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=production nodemon --watch 'server/**' --ext 'ts,json' --exec ts-node --project tsconfig.server.json server/App.ts",
    "server": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development DEV_TYPE=server nodemon --watch 'server/**' --ext 'ts,json' --exec ts-node --project tsconfig.server.json server/App.ts",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json && npm run copy-files",
    "build": "npm run build:next && npm run build:server",
    "start": "cross-env NODE_ENV=production FORCE_COLOR=0 node ./build/server/App.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build"
  },
  "author": "",
  "license": "ISC",
